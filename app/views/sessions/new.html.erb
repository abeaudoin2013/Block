	<div class="row">
		<div class="col-sm-6 login-box">
					<h1 id="login-btn">Log In</h1>
					<div id="login-form">
						<%= form_tag '/sessions', method: :post do %>
							<p>
								<%= label_tag :email %><br>
								<%= text_field_tag :email %>
							</p>
							<p>
								<%= label_tag :password %><br>
								<%= password_field_tag :password_digest %>
							</p>
							<p>
								<%= submit_tag "log in", id: :loginbtn %>
							</p>
						<% end %>
					</div>
		</div>
		<div class="col-sm-6 sign-up-box">

				<%= render 'users/form' %>

		</div>
	</div>
